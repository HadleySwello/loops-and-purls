<div>
  <form class="chart-form">
    <input id="title-input" name="title" placeholder="Title">
    <button type="submit" id="clickMeh" class="btn-default" value="Submit">Submit</button>
    <br></br>
    <table id="pattern-table">
      <tr class="headerRow">
        {{#each headerRow}}
          <td>{{this}}</td>
        {{/each}}
      </tr>
      {{#each cellData}}
        <tr id="{{@key}}">
          <td class="rowLabel">{{this.index}}</td>
          {{#each this.data}}
            <td><input class="cell" id="row{{../this.index}}cell{{@index}}" value="{{this}}" /></td>
          {{/each}}
        </tr>
      {{/each}}
    </table>
  </form>
</div>
